{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "updated": {
        "ClusterId": "employees3",
        "Match": {
          "Path": "/v2/employees/{**catchall}",
          "Methods": [ "POST" ]
        },
        "Transforms": [
          
            { "PathRemovePrefix": "/v2" }
          
        ]
      },
      "mgmt": {
        "ClusterId": "employees2",
        "Match": {
          "Path": "/employees/{id:int}/performance-evals"
        }
      },
      "employees": {
        "ClusterId": "employeesapi",
        "Match": {
          "Path": "/v1/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/v1" }
        ]
      }
    },

    "Clusters": {
      "employeesapi": {
        "Destinations": {
          "employeesapi": {
            "Address": "https://employeesapi"
          }
        }
      },
      "employees3": {
        "Destinations": {
          "employees3": {
            "Address": "https://updatedandnewemployeeapi"
          }
        }
      },
      "employees2": {
        "Destinations": {
          "employees2": {
            "Address": "https://employeesapimanagement"
          }
        }
      }
    }
  }
}
